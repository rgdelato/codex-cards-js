{
  "name": "codex-cards-js",
  "version": "1.0.0",
  "homepage": "http://codexcarddb.com/",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "husky": "^0.13.1",
    "lint-staged": "^3.3.1",
    "prettier": "^0.19.0",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.0.0-beta.6",
    "react-scripts": "0.8.5",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.2"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-autosuggest": "^7.0.1",
    "react-dom": "^15.4.1"
  },
  "scripts": {
    "start": "rm -rf build && node createCardDataJSON.js && react-scripts start",
    "build": "rm -rf build && node createCardDataJSON.js && react-scripts build && cp build/index.html build/200.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && cp CNAME build/CNAME && cd build && surge",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
